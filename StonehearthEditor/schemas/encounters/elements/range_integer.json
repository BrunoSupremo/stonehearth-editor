   "id": "http://stonehearth.net/schemas/encounters/elements/range_integer.json",
   "title": "Integer  Range",
   "description": "A numerical range of integers, from a minimum to a maximum.",
   "type": "object",
   "properties": {
      "min": {"type": "integer"},
      "max": {"type": "integer"}
